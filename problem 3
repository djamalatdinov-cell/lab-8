#include <iostream>
using namespace std;

bool anagram(string s, string t) {
    if (s.length() != t.length())
        return false;

    int count[26] = {0};

    for (int i = 0; i < s.length(); i++) {
    count[s[i] - 'a']++;count[t[i] - 'a']--;
    }

    for (int i = 0; i < 26; i++) {
        if (count[i] != 0)
            return false;
    }

    return true;
}

int main() {
    string s, t;
    cout << "Enter s: ";
    cin >> s;
    cout << "Enter t: ";
    cin >> t;

    if (anagram(s, t))
        cout << "True" << endl;
    else
        cout << "False" << endl;

    return 0;
}
